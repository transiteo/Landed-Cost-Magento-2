<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:DataObject/etc/fieldset.xsd">
    <scope id="global">
        <fieldset id="sales_convert_quote">
            <field name="transiteo_incoterm">
                <aspect name="to_order" />
            </field>
            <field name="base_transiteo_total_taxes">
                <aspect name="to_order" />
            </field>
            <field name="transiteo_total_taxes">
                <aspect name="to_order" />
            </field>
            <field name="base_transiteo_vat">
                <aspect name="to_order" />
            </field>
            <field name="transiteo_vat">
                <aspect name="to_order" />
            </field>
            <field name="base_transiteo_special_taxes">
                <aspect name="to_order" />
            </field>
            <field name="transiteo_special_taxes">
                <aspect name="to_order" />
            </field>
            <field name="base_transiteo_duty">
                <aspect name="to_order" />
            </field>
            <field name="transiteo_duty">
                <aspect name="to_order" />
            </field>
        </fieldset>
        <fieldset id="sales_convert_order">
            <field name="transiteo_incoterm">
                <aspect name="to_order" />
            </field>
            <field name="base_transiteo_total_taxes">
                <aspect name="to_order" />
            </field>
            <field name="transiteo_total_taxes">
                <aspect name="to_order" />
            </field>
            <field name="base_transiteo_vat">
                <aspect name="to_order" />
            </field>
            <field name="transiteo_vat">
                <aspect name="to_order" />
            </field>
            <field name="base_transiteo_special_taxes">
                <aspect name="to_order" />
            </field>
            <field name="transiteo_special_taxes">
                <aspect name="to_order" />
            </field>
            <field name="base_transiteo_duty">
                <aspect name="to_order" />
            </field>
            <field name="transiteo_duty">
                <aspect name="to_order" />
            </field>
        </fieldset>
        <fieldset id="quote_convert_item">
            <field name="base_transiteo_total_taxes">
                <aspect name="to_order_item" />
            </field>
            <field name="transiteo_total_taxes">
                <aspect name="to_order_item" />
            </field>
            <field name="base_transiteo_vat">
                <aspect name="to_order_item" />
            </field>
            <field name="transiteo_vat">
                <aspect name="to_order_item" />
            </field>
            <field name="base_transiteo_special_taxes">
                <aspect name="to_order_item" />
            </field>
            <field name="transiteo_special_taxes">
                <aspect name="to_order_item" />
            </field>
            <field name="base_transiteo_duty">
                <aspect name="to_order_item" />
            </field>
            <field name="transiteo_duty">
                <aspect name="to_order_item" />
            </field>
        </fieldset>
        <fieldset id="sales_convert_order_item">
            <field name="base_transiteo_total_taxes">
                <aspect name="to_quote_item" />
                <aspect name="to_invoice_item" />
                <aspect name="to_cm_item" />
            </field>
            <field name="transiteo_total_taxes">
                <aspect name="to_quote_item" />
                <aspect name="to_invoice_item" />
                <aspect name="to_cm_item" />
            </field>
            <field name="base_transiteo_vat">
                <aspect name="to_quote_item" />
                <aspect name="to_invoice_item" />
                <aspect name="to_cm_item" />
            </field>
            <field name="transiteo_vat">
                <aspect name="to_quote_item" />
                <aspect name="to_invoice_item" />
                <aspect name="to_cm_item" />
            </field>
            <field name="base_transiteo_special_taxes">
                <aspect name="to_quote_item" />
                <aspect name="to_invoice_item" />
                <aspect name="to_cm_item" />
            </field>
            <field name="transiteo_special_taxes">
                <aspect name="to_quote_item" />
                <aspect name="to_invoice_item" />
                <aspect name="to_cm_item" />
            </field>
            <field name="base_transiteo_duty">
                <aspect name="to_quote_item" />
                <aspect name="to_invoice_item" />
                <aspect name="to_cm_item" />
            </field>
            <field name="transiteo_duty">
                <aspect name="to_quote_item" />
                <aspect name="to_invoice_item" />
                <aspect name="to_cm_item" />
            </field>
        </fieldset>
    </scope>
</config>
